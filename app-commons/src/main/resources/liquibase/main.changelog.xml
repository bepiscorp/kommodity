<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.9.xsd">

    <includeAll
            path="liquibase/tyrell"
            filter="tyrell.callisto.data.liquibase.IncludeAllSchemaTyrellChangelogFilter"
            resourceComparator="tyrell.callisto.data.liquibase.LogicalResourceComparator"
            errorIfMissingOrEmpty="false"
    />

    <includeAll
            path="liquibase/tyrell"
            filter="tyrell.callisto.data.liquibase.IncludeAllDataTyrellChangelogFilter"
            resourceComparator="tyrell.callisto.data.liquibase.LogicalResourceComparator"
            errorIfMissingOrEmpty="false"
    />
</databaseChangeLog>
